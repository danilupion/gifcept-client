    ]
  },
  "dependencies": {
    "jwt-decode": "^2.2.0",
    "lodash": "^4.17.15",
    "mobx": "^5.13.1",
    "mobx-react": "^6.1.3",
    "moment": "^2.24.0",
    "react": "^16.10.1",
    "react-dom": "^16.10.1",
    "react-modal": "^3.10.1",
    "react-router-dom": "^5.1.2",
    "url": "^0.11.0"
  },
  "devDependencies": {
    "@types/facebook-js-sdk": "^3.3.0",
    "@types/jest": "24.0.18",
    "@types/jwt-decode": "^2.2.1",
    "@types/lodash": "^4.14.141",
    "@types/moment": "^2.13.0",
    "@types/node": "12.7.8",
    "@types/react": "16.9.4",
    "@types/react-dom": "16.9.1",
    "@types/react-modal": "^3.8.3",
    "@types/react-router-dom": "^5.1.0",
    "lint-staged": "^9.4.1",
    "node-sass-chokidar": "^1.3.5",
    "prettier": "^1.18.2",
    "react-scripts": "3.1.2",
    "typescript": "3.6.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "test": "react-scripts test",
    "build": "yarn build-css && react-scripts build",
    "build-css": "node-sass-chokidar src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive",
    "pretty": "prettier --write",
    "pretty-all": "yarn pretty \"{src,__tests__}/**/*.{ts,tsx}\"",
    "lint": "tslint",
    "lint-all": "yarn lint \"{src,__tests__}/**/*.{ts,tsx}\"",
    "precommit": "lint-staged && yarn test"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
